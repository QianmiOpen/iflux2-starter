{
  "name": "iflux2-starter",
  "version": "1.0.0",
  "description": "a baisc bootstrap project for iflux2",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "./node_modules/.bin/webpack-dev-server --port 3000",
    "clean": "rm -rf ./dist",
    "build:dll": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.dll.config.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.production.js --progress --color",
    "build:all": "NODE_ENV=production npm run clean && npm run build:dll && npm run build"
  },
  "keywords": [
    "iflux2",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brothers-js/iflux2-web-boot.git"
  },
  "author": "hufeng",
  "license": "BSD",
  "dependencies": {
    "classnames": "^2.2.5",
    "iflux2": "1.2.6",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "whatwg-fetch": "2.0.0"
  },
  "devDependencies": {
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "html-webpack-plugin": "2.24.1",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "1.16.2"
  }
}
